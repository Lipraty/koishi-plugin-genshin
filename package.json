      "zh": "用于Koishi的某动漫游戏插件"
    },
    "preview": true,
    "service": {
      "required": [
        "database",
        "puppeteer"
      ],
      "implements": [
        "paimon"
      ]
    }
  },
  "peerDependencies": {
    "koishi": "^4.11.1",
    "koishi-plugin-puppeteer": "^3.0.2"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "lodash.isequal": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@koishijs/plugin-help": "^1.2.6",
    "@types/uuid": "^8.3.4"
  }
